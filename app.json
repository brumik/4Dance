{
  "expo": {
    "name": "4Dance",
    "slug": "4Dance",
    "privacy": "public",
    "description": "A companion app for dance schools!",
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#FABE59",
    "icon": "./assets/iconNotAdaptive.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FABE59"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "me.berky.forDance",
      "versionCode": 1,
      "permissions": [],
      "icon": "./assets/iconNotAdaptive.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FABE59"
      },
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./assets/iconNotAdaptive.png"
    }
  }
}
